{
  "name": "raneda",
  "version": "1.0.0",
  "description": "Raneda Website",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ickynavigator/raneda.git"
  },
  "author": "Obi Fortune",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ickynavigator/raneda/issues"
  },
  "homepage": "https://github.com/ickynavigator/raneda#readme",
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.34",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.25.1",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "emailjs": "^3.4.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "firebase-admin": "^9.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.2.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.20"
  }
}
